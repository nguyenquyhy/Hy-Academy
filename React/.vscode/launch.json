{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "presentation": {
                "hidden": false,
                "group": "1",
                "order": 1
            },
            "request": "launch",
            "runtimeArgs": [
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "Edge debug",
            "presentation": {
                "hidden": false,
                "group": "2",
                "order": 2
            },
            "type": "pwa-msedge",
            "request": "launch",
            "url": "https://localhost:3000",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Edge (Dev) debug",
            "presentation": {
                "hidden": false,
                "group": "2",
                "order": 2
            },
            "type": "pwa-msedge",
            "runtimeExecutable": "dev",
            "request": "launch",
            "url": "https://localhost:3000",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Chrome debug",
            "presentation": {
                "hidden": false,
                "group": "2",
                "order": 2
            },
            "type": "pwa-chrome",
            "request": "launch",
            "url": "https://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}